{
  "name": "@wixc3/wcs-core",
  "version": "1.4.4",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "scripts": {
    "clean": "rimraf cjs esm",
    "prebuild": "yarn clean",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir cjs",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir esm",
    "typecheck": "tsc --noEmit",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "devDependencies": {
    "@types/react": "^16.9.23",
    "react": "^16.13.0",
    "rimraf": "^3.0.2",
    "typescript": "~3.8.3"
  },
  "repository": "https://github.com/wixplosives/wcs-core"
}
